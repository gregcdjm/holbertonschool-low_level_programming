#include "main.h"
#include <stdio.h>

/**
 * print_to_98 - check if it's lower
 * @n: 1st ereg
 * Return: return
 */

void print_to_98(int n)
{
	if (n == 98)
	{
		_putchar(98 / 10 + 48);
		_putchar(98 % 10 + 48);
	}
	if (n < 98)
	{
		for (n = n; n <= 98; n++)
		{
			_putchar(n / 10 + 48);
			_putchar(n % 10 + 48);
		}
	}
	if (n > 98)
	{
		for (n = n; n >= 98; n--)
		{
			_putchar(n / 10 + 48);
			_putchar(n % 10 + 48);
		}
	}
}
