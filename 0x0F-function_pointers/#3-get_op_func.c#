#include <stdio.h>
#include "3-calc.h"
#include <stdlib.h>
/**
 * print_all - check if a number is equal to 98
 * @s: the integer to check
 * Return: a+b
 */
void print_all(const char * const format, ...);
{
	op_t ops[] = {
		{"+", op_add},
		{"-", op_sub},
		{"*", op_mul},
		{"/", op_div},
		{"%", op_mod},
		{NULL, NULL}
	};
	int i = 0;

	while (ops[i].op != NULL && *(ops[i].op) != *s)
		i++;
	return (ops[i].f);
}
